{
  "name": "indomitable",
  "version": "0.0.3",
  "description": "A lightweight (the actual ship is heavy though), performat & powerful sharder for Discord.JS. Indomitable uses cluster module to evenly spread her weight (load) across your cores",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc && npm run build:esm && npm run build:docs",
    "build:esm": "gen-esm-wrapper dist/index.js dist/index.mjs",
    "build:docs": "typedoc --theme default --readme README.md --out docs/ --entryPointStrategy expand src/.",
    "prepare": "tsc"
  },
  "keywords": [
    "cluster",
    "clustering",
    "sharder",
    "sharding",
    "discord",
    "discord sharder",
    "discord.js",
    "discord.js sharder",
    "discordjs",
    "discordjs sharder",
    "bot"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deivu/Indomitable.git"
  },
  "author": "Saya (Deivu)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Deivu/Indomitable/issues"
  },
  "homepage": "https://github.com/Deivu/Indomitable#readme",
  "dependencies": {
    "net-ipc": "^2.0.1"
  },
    "peerDependencies": {
      "discord.js": "~13 || ~14"
    },
  "optionalDependencies": {
    "fast-zlib": "^2.0.1",
    "msgpackr": "^1.3.2"
  },
  "devDependencies": {
    "@augu/eslint-config": "^2.2.0",
    "@types/node": "^17.0.43",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "gen-esm-wrapper": "^1.1.3",
    "ts-node": "^10.8.1",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.3"
  }
}
